@model checkintegration.Models.AccidentInfo
@{
    Layout = null;
}

@using (Html.BeginForm("UploadReport", "Admin", FormMethod.Post, new { @enctype = "multipart/form-data" }))
{
    @Html.HiddenFor(model => model.AccidentId, new { @name="acci"})


    
    <div class="form-group">
        Report Type:
      <input type="text" class="form-control" name="file1" required/>

    </div>
    
    <div class="form-group">
         File Upload:
         <input type='file' id="file" name="file"/>
     </div> <!-- Allowed to select Multiple files at a stretch-->
   
   
    
   
    <div class="form-group">
        <input type="submit" value="Upload" class="btn btn-primary" />

    </div>
}