@model checkintegration.Models.Registration
@{
    Layout = null;
}


@using (Html.BeginForm("AddorEditCustomer", "Admin", FormMethod.Post, new { onsubmit = "return SubmitForm(this)" }))
{
    @Html.HiddenFor(model => model.UserID)


    <div class="form-group">
        FirstName:
        @Html.EditorFor(model => model.FirstName, new { htmlAttributes = new { @class = "form-control1" } })
        @Html.ValidationMessageFor(model => model.FirstName)

    </div>
    <div class="form-group">
        LastName:
        @Html.EditorFor(model => model.LastName, new { htmlAttributes = new { @class = "form-control1" } })
        @Html.ValidationMessageFor(model => model.LastName)

    </div>
    <div class="form-group">
        UserName:
        @Html.EditorFor(model => model.UserName, new { htmlAttributes = new { @class = "form-control1" } })
        @Html.ValidationMessageFor(model => model.UserName)

    </div>
    <div class="form-group">
        EmailId:
        @Html.EditorFor(model => model.EmailId, new { htmlAttributes = new { @class = "form-control1" } })
        @Html.ValidationMessageFor(model => model.EmailId)

    </div>
    <div class="form-group">
        Address:
        @Html.EditorFor(model => model.Address, new { htmlAttributes = new { @class = "form-control1" } })
        @Html.ValidationMessageFor(model => model.Address)

    </div>
    <div class="form-group">
        Pincode:
        @Html.EditorFor(model => model.Pincode, new { htmlAttributes = new { @class = "form-control1" } })
        @Html.ValidationMessageFor(model => model.Pincode)

    </div>
    <div class="form-group">
        Phone No:
        @Html.EditorFor(model => model.PhoneNumber, new { htmlAttributes = new { @class = "form-control1" } })
        @Html.ValidationMessageFor(model => model.PhoneNumber)

    </div>
    <div class="form-group">
        <input type="submit" value="Submit" class="btn btn-primary" />

    </div>
}